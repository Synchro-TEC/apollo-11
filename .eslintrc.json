{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": ["prettier", "plugin:react/recommended"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "experimentalDecorators": true,
            "jsx": true
        },
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": ["error", {"tabWidth": 2 ,"singleQuote": true, "printWidth": 120, "trailingComma": "es5"}]
    }
}  